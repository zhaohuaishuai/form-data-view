{
  "name": "vite-react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config ./config/vite.dev.react.js",
    "dev:react": "vite --config ./config/vite.dev.react.js",
    "dev:vue": "vite --config ./config/vite.dev.vue.js",
    "build:react": "vite build --config ./config/vite.prod.react.js",
    "build:vue": "vite build --config ./config/vite.prod.vue.js",
    "build": "vite build",
